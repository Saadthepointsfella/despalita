{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://maxmin.agency/schemas/examples/answers_example.schema.json",
  "type": "object",
  "required": [
    "version",
    "answers"
  ],
  "additionalProperties": false,
  "properties": {
    "version": {
      "type": "string"
    },
    "answers": {
      "type": "array",
      "minItems": 24,
      "maxItems": 24,
      "items": {
        "type": "object",
        "required": [
          "question_id",
          "option_id"
        ],
        "additionalProperties": false,
        "properties": {
          "question_id": {
            "type": "string"
          },
          "option_id": {
            "type": "string"
          },
          "time_spent_ms": {
            "type": "integer",
            "minimum": 0
          }
        }
      }
    }
  }
}
